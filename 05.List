void main(){
  List names = ["Carlos","Lucia","Lolito"];
  print (names);
  
  // la posicion siempre comienza desde 0
  print(names[0]);
  
  // agregar valores a la lista con el metodo add
  names.add("Saori");
  print (names);
  
  // Eliminar 1 valor de la lista por posicion
  names.removeAt(2);
  print (names);
  
  // Eliminar 1 valor de la lista declarando el elemento
  names.remove("Carlos");
  print(names);
  
  // Creacion de una lista que no cambie los valores que tiene se agrega const
  const List apellidos = ["Ramos", "Cacerez", "Tupayachi"];
  print(apellidos);
  // si intentamos agregar o eliminar los elementos de la lista nos arrojara un error.
  //apellidos.add("Chique");
  // print(apellidos);
  
  // saber la cantidad de elemntos de la lista.
  print("El tama√±o de la lista es: ${apellidos.length}");
  
}
